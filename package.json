{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Minimal and Optimized React / Webpack Boilerplate",
  "main": "index.js",
  "scripts": {
    "prod": " webpack -- --env.mode production",
    "dev": " webpack-dev-server --open -- --env.mode development --hot",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "prod:debug": " debug -- --env.mode production",
    "dev:debug": " debug -- --env.mode development",
    "prod:analyze": " prod -- --env.presets analyze"
  },
  "repository": "https://github.com/idrismakesgames/react-boilerplate.git",
  "author": "Idris Hussain <idrismakesgames@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
